https://practice.geeksforgeeks.org/problems/palindrome-string0817/1


Palindrome String

class Solution{
public:	
	
	
	int isPalindrome(string S)
	{
	    // Your code goes here
	    
	    int n=S.size();
	    for(int i=0;i<n;i++)

{
    if(S[i]==S[n-1-i])
    {
        continue;
        
    }else
    {
        return 0;
    }
}
return 1;}
